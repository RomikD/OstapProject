{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off",
    "operator-linebreak": "off",
    "no-invalid-this": "off",
    "linebreak-style": 0
  },
  "extends": "google"
}
